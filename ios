#include <stdio.h>
#include <stdlib.h>

int main() {
    int gatdula_num, i;
    int *gatdula_numbers;

    printf("Enter number of elements: ");
    scanf("%d", &gatdula_num);

    gatdula_numbers = (int*) malloc(gatdula_num * sizeof(int));

    if (gatdula_numbers == NULL) {
        printf("Memory allocation failed.\n");
        return 1;
    }

    for (i = 0; i < gatdula_num; i++) {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &gatdula_numbers[i]);
    }

    printf("\nYou entered:\n");
    for (i = 0; i < gatdula_num; i++) {
        printf("%d ", gatdula_numbers[i]);
    }

    free(gatdula_numbers);

    return 0;
}
